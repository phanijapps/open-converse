    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^9.2.0",
    "ts-loader": "^9.5.2",
